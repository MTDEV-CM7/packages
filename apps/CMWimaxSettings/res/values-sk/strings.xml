    <string name="wimax_status_disconnecting">Odpájanie\u2026</string>
    <!-- Status message of wimax when it is disconnecting from a network -->
    <string name="wimax_status_disconnecting_from">odpojené od  <xliff:g id="network_name">%1$s</xliff:g>\u2026</string>
